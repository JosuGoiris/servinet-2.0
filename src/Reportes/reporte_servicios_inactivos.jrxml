	<queryString>
		<![CDATA[select sc.idServiciodelCliente, c.idCliente, p.nombres, p.apellidos, p.cedulaIdent, s.idServicio, s.nombreServicio, ds.precio, sc.fechaInicio, sc.fechaPago, sc.estado from tblserviciodelcliente as sc inner join tblcliente as c on sc.clienteId = c.idCliente inner join tblpersona as p on c.personaId = p.idPersona inner join tbldetalleservicio as ds on sc.detalleServicioId = ds.idDetalleServicio inner join tblservicio as s on ds.servicioId = s.idServicio where sc.estado = $P{estado}]]>
