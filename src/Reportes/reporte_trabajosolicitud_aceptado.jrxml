	<queryString>
		<![CDATA[select ts.idTrabajosSolicitud, sd.idSolicitud, c.idCliente, p.nombres, p.apellidos, p.cedulaIdent, sd.fechaSolicitud, sd.fechaActualizado, s.idServicio, s.nombreServicio, cd.idCuadrilla, cd.nombreCuadrilla, sd.estado from tbltrabajossolicitud as ts inner join tblsolicitud as sd on ts.solicitudId = sd.idSolicitud inner join tblcliente as c on sd.clienteId = c.idCliente inner join tblpersona as p on c.personaId = p.idPersona inner join tblservicio as s on sd.servicioId = s.idServicio inner join tblcuadrilla as cd on ts.cuadrillaId = cd.idCuadrilla inner join tbltrabajos as t on ts.idTrabajosSolicitud = t.trabajossolicitudId where sd.estado = $P{estado}]]>
	</queryString>
